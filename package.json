{
  "name": "recharts",
  "version": "0.5.2",
  "description": "React charts",
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "files": [
    "*.md",
    "demo",
    "es6",
    "lib",
    "umd",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "chart",
    "react-component"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6 && rimraf umd && npm run build-umd && npm run build-min",
    "build-cjs": "rimraf lib && babel ./src -d lib",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules",
    "build-umd": "NODE_ENV=production webpack src/index.js umd/Recharts.js",
    "build-min": "NODE_ENV=production webpack -p src/index.js umd/Recharts.min.js",
    "demo": "webpack-dev-server  --progress --port 3000  --colors --content-base demo --hot --inline --config demo/webpack.config.js",
    "test": "./node_modules/karma/bin/karma start test/karma.conf.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/recharts/recharts.git"
  },
  "author": "recharts group",
  "bugs": {
    "url": "https://github.com/recharts/recharts/issues"
  },
  "homepage": "https://github.com/recharts/recharts",
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-addons-transition-group": "^0.14.0"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "core-js": "2.0.3",
    "d3-scale": "0.4.0",
    "d3-shape": "0.4.0",
    "ima.js-babel6-polyfill": "0.1.0",
    "invariant": "2.2.0",
    "lodash": "^4.2.1",
    "oui-dom-utils": "0.3.5",
    "pure-render-decorator": "0.2.0",
    "react-smooth": "0.0.7",
    "recharts-scale": "0.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.3.21",
    "babel-core": "^6.3.21",
    "babel-eslint": "5.0.0-beta10",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^2.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "enzyme": "^1.4.1",
    "escope": "^3.3.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "history": "^1.17.0",
    "isparta": "^4.0.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-libs-browser": "^0.5.3",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-addons-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.3",
    "require-dir": "^0.3.0",
    "sinon": "^1.17.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "license": "MIT"
}
