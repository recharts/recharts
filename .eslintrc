{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "ecmaFeatures": {
    "jsx": true,
    "objectLiteralShorthandMethods": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [0],
    "new-parens": [1],
    "no-alert": [1],
    "handle-callback-err": [1],
    "no-extra-strict": [1],
    "no-script-url": [0],
    "space-unary-ops": [0],
    "consistent-return": [0],
    "comma-dangle": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-multi-spaces": [0],
    "no-unused-vars": [0],
    "key-spacing": [0],
    "no-empty": [0],
    "no-shadow": [0],
    "no-use-before-define": [0],
    "no-unused-expressions": [0],
    "no-new-func": [0],
    "new-cap": [0],
    "eqeqeq": [0],
    "curly": [0],
    "strict": [0],
    "no-new": [1],
    "eol-last": [1],
    "space-infix-ops": [1],
    "no-return-assign": [1],
    "comma-spacing": [1],
    "no-extra-boolean-cast": [1],
    "no-constant-condition": [1]
  },
  "globals": {
    "describe": false,
    "chai": false,
    "beforeEach": false,
    "afterEach": false,
    "it": false
  }
}
