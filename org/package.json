{
  "name": "org",
  "version": "0.0.1",
  "description": "the website of recharts",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.prod.js && npm run sync-examples",
    "sync-examples": "rm -rf docs/examples/ && cp -r src/docs/exampleComponents/ docs/examples/",
    "sync-examples-dev": "rm -rf public/examples/ && cp -r src/docs/exampleComponents/ public/examples/",
    "start": "npm run sync-examples-dev && webpack serve --config webpack.config.dev.js",
    "autofix": "eslint src --fix",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*.js",
    "gh-pages:404": "cp build/index.html build/404.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/recharts/recharts.org.git"
  },
  "eslintConfig": {
    "eslintIgnore": [
      "./docs/**"
    ]
  },
  "author": "recharts group",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "6.12.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^1.3.0",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "prettier": "2.1.2",
    "react-hot-loader": "^4.13.0",
    "redux-devtools": "^3.7.0",
    "redux-devtools-dock-monitor": "^1.2.0",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.0",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-fetch": "^3.1.2",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^1.3.3",
    "d3-shape": "^2.0.0",
