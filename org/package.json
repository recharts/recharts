    "react-monaco-editor": "0.40.0",
    "react-redux": "^7.0.0",
    "react-router": "^3.0.0",
    "react-router-redux": "~4.0.7",
    "react-router-scroll": "~0.4.1",
    "react-runner": "^1.0.3",
    "recharts": "^2.3.2",
    "redux": "4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.5.4",
    "scroll-behavior": "^0.9.12",
    "simple-line-icons": "^2.5.5"
  },
  "license": "MIT"
}
